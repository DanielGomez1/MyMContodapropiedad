                			<radiogroup>
                			<radio label="Venta"/>
                			<radio label="Arriendo"/>
                			</radiogroup>
                		</hlayout>
                		<separator/>
                		<label value="Tipo de propiedad:  " />
                			<radiogroup orient="vertical">
                			<radio label="Casa"/>
                			<radio label="Apartemento"/>
                			<radio label="Local"/>
                			<radio label="lote"/>
                			</radiogroup>
                		<separator/>                 			
                		<textbox value="Precio Maximo"/>
                		<textbox value="Precio Minimo"/>
                		<textbox value="Codigo"/>
                		<separator/>
                		<button label="Buscar" onClick="" style="background:#8BC34A" width="100%"/>	
                	</vlayout>
                </groupbox>
        </west>
      <south>
