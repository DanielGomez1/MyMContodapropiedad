								<attribute name="onClick">
									Executions.sendRedirect("Propiedades.zul");
								</attribute>
							</menuitem>
							<menuitem image="/Iconos/building78.png" label="${labels.lblApartamentos}" onClick="alert(self.label)" style="background:#FF9800">
								<attribute name="onClick">
									Executions.sendRedirect("Propiedades.zul");
								</attribute>
							</menuitem>
							<menuitem image="/Iconos/store.png" label="${labels.lblLocales}" onClick="alert(self.label)" style="background:#FF9800">
								<attribute name="onClick">
									Executions.sendRedirect("Propiedades.zul");
								</attribute>
							</menuitem>
							<menuitem image="/Iconos/tree.png" label="${labels.lblLotes}" onClick="alert(self.label)" style="background:#FF9800">
								<attribute name="onClick">
									Executions.sendRedirect("Propiedades.zul");
								</attribute>
