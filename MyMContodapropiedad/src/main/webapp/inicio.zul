</hlayout>

</north>
<center>	
<groupbox> 
	<grid>
		<columns>
			<column label="" width="30%"/>
			<column label="" width="70%"/>
		</columns>
		<rows>
			<row>	
			<image src="/Imagenes/casa.jpg" />
			<label value="Ventas" />
			</row>
		</rows>
	</grid>
				
	
</groupbox> 
